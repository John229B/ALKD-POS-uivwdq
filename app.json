{
  "expo": {
    "name": "ALKD-POS",
    "slug": "ALKD-POS",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/e4a5b0f6-595e-41ee-86a1-85f554302eee.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/e4a5b0f6-595e-41ee-86a1-85f554302eee.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#FFC107"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alkd.pos",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/e4a5b0f6-595e-41ee-86a1-85f554302eee.jpeg",
        "backgroundColor": "#FFC107"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alkd.pos",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/images/e4a5b0f6-595e-41ee-86a1-85f554302eee.jpeg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "alkd-pos",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}